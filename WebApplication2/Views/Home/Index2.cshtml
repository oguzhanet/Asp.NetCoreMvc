@model List<WebApplication2.Models.Book>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index2</title>
</head>
<body>
    <table>
        @foreach (var item in Model)
        {
          <tr>
              <th>@item.Id</th>
              <td>@item.BookName</td>
              <td>@item.Writer</td>

          </tr>

        }
    </table>
</body>
</html>
